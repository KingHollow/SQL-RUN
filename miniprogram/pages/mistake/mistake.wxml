<!--pages/mistake/mistake.wxml-->
<view class="title">我的错题</view>

<view class="container">
  <view class="content">
  <view wx:for="{{list}}" wx:key="index">
    <view view class="content" catchtap="onTapNavigateTo" data-id="{{item.questionID}}" data-type="{{item.type}}" style="padding:2vh 2vh 2vh 2vh">
      <view class = "name" space="emsp">题型：{{item.style}}     {{item.chapter}}</view>
      <text class="text">{{item.content}}</text>

    </view>
    <view class="line"></view>
  </view>
</view>
</view>



